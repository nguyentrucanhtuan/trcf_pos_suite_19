<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Inventory Check -->
        <record id="seq_trcf_inventory_check" model="ir.sequence">
            <field name="name">TRCF Inventory Check Sequence</field>
            <field name="code">trcf.inventory.check</field>
            <field name="prefix">INV/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
